	<bean id="KDBCLUSTER1" class="com.citi.ocean.restapi.datasource.base.Cluster">
		<constructor-arg name="name" value="KDBCLUSTER1" />
		<constructor-arg name="type" value="KDB" />
		<constructor-arg name="shards">
			<list>
				<bean class="com.citi.ocean.restapi.datasource.base.KDBShard">
					<property name="topic" value="kdbcluster1_shard1_query_topic"/>
					<property name="endpoints">
						<list>
							<bean class="com.citi.ocean.restapi.datasource.KDBEndpoint">
								<property name="host" value="fxomdlnx3p.nam.nsroot.net" />
								<property name="port" value="5555" />
								<property name="user" value="rdb" />
								<property name="password" value="rdb" />
								<property name="connectionsCount" value="1" />
							</bean>
							<bean class="com.citi.ocean.restapi.datasource.KDBEndpoint">
								<property name="host" value="fxomdlnx3c.nam.nsroot.net" />
								<property name="port" value="5555" />
								<property name="user" value="rdb" />
								<property name="password" value="rdb" />
								<property name="connectionsCount" value="1" />
							</bean>
						</list>
					</property>
					<property name="sharedCriteria">
						<map>
							<entry key="TRD_DATE">
								<list>
									<value>-1M</value>
									<value>-2M</value>
								</list>
							</entry>
						</map>
					</property>
				</bean>
				<bean class="com.citi.ocean.restapi.datasource.base.KDBShard">
					<property name="topic" value="kdbcluster1_shard2_query_topic"/>
					<property name="endpoints">
						<list>
							<bean class="com.citi.ocean.restapi.datasource.KDBEndpoint">
								<property name="host" value="fxomdlnx3p.nam.nsroot.net" />
								<property name="port" value="6666" />
								<property name="user" value="rdb" />
								<property name="password" value="rdb" />
								<property name="connectionsCount" value="1" />
							</bean>
							<bean class="com.citi.ocean.restapi.datasource.KDBEndpoint">
								<property name="host" value="fxomdlnx3c.nam.nsroot.net" />
								<property name="port" value="6666" />
								<property name="user" value="rdb" />
								<property name="password" value="rdb" />
								<property name="connectionsCount" value="1" />
							</bean>
						</list>
					</property>
					<property name="sharedCriteria">
						<map>
							<entry key="TRD_DATE">
								<list>
									<value>-3M</value>
									<value>-4M</value>
								</list>
							</entry>
						</map>
					</property>
				</bean>
			</list>
		</constructor-arg>
	</bean>

